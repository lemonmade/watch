{
  "name": "watch",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "quilt lint",
    "test": "quilt test",
    "type-check": "quilt type-check",
    "develop": "quilt develop",
    "build": "quilt build",
    "packages:version": "changeset version && pnpm install --no-frozen-lockfile",
    "packages:snapshot": "changeset version --snapshot",
    "packages:deploy": "pnpm -r publish",
    "cdk": "pnpm --filter infrastructure run cdk"
  },
  "engines": {
    "node": ">=14.18.0"
  },
  "packageManager": "pnpm@6.32.2",
  "dependencies": {
    "@quilted/quilt": "^0.5.93",
    "react": "npm:@quilted/react@^0.1.2",
    "react-dom": "npm:@quilted/react@^0.1.2"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.3",
    "@changesets/cli": "^2.21.1",
    "@prisma/client": "^3.11.0",
    "@prisma/engines": "^3.11.0-0",
    "@quilted/aws": "^0.3.36",
    "@quilted/browserslist-config": "^0.1.6",
    "@quilted/craft": "^0.1.111",
    "@quilted/eslint-config": "^0.1.9",
    "@quilted/prettier": "^0.2.8",
    "@quilted/typescript": "^0.2.11",
    "@types/node": "^16.11.0",
    "esbuild": "^0.14.0",
    "esbuild-register": "^3.3.2",
    "prisma": "^3.11.0",
    "typescript": "^4.6.0"
  },
  "prettier": "@quilted/prettier",
  "eslintConfig": {
    "extends": "@quilted/eslint-config/workspace",
    "ignorePatterns": [
      "node_modules/",
      "build/",
      "cdk.out/",
      "app/graphql/types*",
      "**/*.graphql.d.ts",
      "**/*.graphql.ts",
      "packages/cli/src/commands/develop/schema.ts"
    ],
    "rules": {
      "@typescript-eslint/ban-type": "off"
    }
  },
  "browserslist": [
    "extends @quilted/browserslist-config/defaults"
  ],
  "pnpm": {
    "overrides": {
      "@types/react": "^17.0.0"
    }
  }
}
