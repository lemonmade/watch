{
  "name": "watch",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "sewing-kit lint",
    "test": "sewing-kit test",
    "type-check": "sewing-kit type-check",
    "develop": "sewing-kit develop",
    "build": "sewing-kit build",
    "packages:version": "lerna version --include-merged-tags --no-private",
    "packages:publish": "lerna publish from-package --yes --no-verify-access --no-private",
    "cdk": "cdk --app \"babel-node --presets=@babel/preset-typescript,@babel/preset-env --extensions .ts,.tsx,.mjs,.js,.json ./config/cdk/index.ts\""
  },
  "workspaces": {
    "packages": [
      "app",
      "functions/*",
      "packages/*"
    ]
  },
  "dependencies": {
    "@quilted/http-handlers": "^0.2.10"
  },
  "devDependencies": {
    "@aws-cdk/aws-apigatewayv2": "^1.96.0",
    "@aws-cdk/aws-apigatewayv2-integrations": "^1.96.0",
    "@aws-cdk/aws-cloudfront": "^1.95.1",
    "@aws-cdk/aws-cloudfront-origins": "^1.95.1",
    "@aws-cdk/aws-ec2": "^1.95.1",
    "@aws-cdk/aws-events": "^1.95.1",
    "@aws-cdk/aws-events-targets": "^1.96.0",
    "@aws-cdk/aws-iam": "^1.95.1",
    "@aws-cdk/aws-lambda": "^1.95.1",
    "@aws-cdk/aws-lambda-event-sources": "^1.96.0",
    "@aws-cdk/aws-rds": "^1.95.1",
    "@aws-cdk/aws-route53": "^1.95.1",
    "@aws-cdk/aws-route53-targets": "^1.96.0",
    "@aws-cdk/aws-s3": "^1.95.1",
    "@aws-cdk/aws-s3-deployment": "^1.96.0",
    "@aws-cdk/core": "^1.95.1",
    "@babel/node": "^7.10.3",
    "@prisma/client": "^2.21.2",
    "@quilted/aws": "^0.3.15",
    "@quilted/craft": "^0.1.36",
    "@quilted/sewing-kit": "^0.2.11",
    "@types/common-tags": "^1.8.0",
    "@types/fs-extra": "^9.0.11",
    "@types/isomorphic-form-data": "^2.0.0",
    "aws-cdk": "^1.96.0",
    "aws-sdk": "^2.878.0",
    "common-tags": "^1.8.0",
    "dotenv": "^8.2.0",
    "enhanced-resolve": "^5.8.0",
    "lerna": "^3.22.1",
    "prisma": "^2.21.2",
    "typescript": "^4.3.0"
  },
  "prettier": "@quilted/prettier",
  "eslintConfig": {
    "extends": "@quilted/eslint-config/workspace",
    "ignorePatterns": [
      "node_modules/",
      "build/",
      "cdk.out/",
      "app/graphql/types*",
      "**/*.graphql.d.ts",
      "**/*.graphql.ts"
    ],
    "rules": {
      "@typescript-eslint/ban-type": "off"
    }
  }
}
