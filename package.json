{
  "name": "watch",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "sewing-kit lint",
    "test": "sewing-kit test",
    "type-check": "sewing-kit type-check",
    "develop": "sewing-kit develop",
    "build": "sewing-kit build",
    "packages:version": "lerna version --include-merged-tags --no-private",
    "packages:publish": "lerna publish from-package --yes --no-verify-access --no-private",
    "cdk": "pnpm --filter infrastructure run cdk"
  },
  "packageManager": "pnpm@6.32.2",
  "dependencies": {
    "@quilted/quilt": "^0.5.74",
    "react": "npm:@quilted/react@^0.1.2",
    "react-dom": "npm:@quilted/react@^0.1.2"
  },
  "devDependencies": {
    "@prisma/client": "^2.25.0",
    "@quilted/aws": "^0.3.35",
    "@quilted/browserslist-config": "^0.1.5",
    "@quilted/craft": "^0.1.78",
    "@quilted/eslint-config": "^0.1.8",
    "@quilted/prettier": "^0.2.7",
    "@quilted/sewing-kit": "^0.2.23",
    "@quilted/sewing-kit-rollup": "^0.1.15",
    "@quilted/sewing-kit-vite": "^0.1.11",
    "@quilted/typescript": "^0.2.9",
    "@types/fs-extra": "^9.0.11",
    "lerna": "^3.22.1",
    "prisma": "^2.25.0",
    "typescript": "^4.6.0"
  },
  "prettier": "@quilted/prettier",
  "eslintConfig": {
    "extends": "@quilted/eslint-config/workspace",
    "ignorePatterns": [
      "node_modules/",
      "build/",
      "cdk.out/",
      "app/graphql/types*",
      "**/*.graphql.d.ts",
      "**/*.graphql.ts",
      "packages/cli/src/commands/develop/schema.ts"
    ],
    "rules": {
      "@typescript-eslint/ban-type": "off"
    }
  },
  "browserslist": [
    "extends @quilted/browserslist-config/defaults"
  ]
}
