{
  "name": "watch",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "sk lint",
    "test": "sk test",
    "type-check": "sk type-check",
    "dev": "sk dev",
    "build": "sk build",
    "packages:version": "lerna version --no-private",
    "packages:publish": "lerna publish from-package --yes --no-verify-access --no-private",
    "cdk": "babel-node --extensions .ts,.tsx,.mjs,.js,.json ./scripts/cdk.ts",
    "run:ts": "babel-node --extensions .ts,.tsx,.mjs,.js,.json"
  },
  "workspaces": {
    "packages": [
      "app",
      "functions/*",
      "packages/*"
    ]
  },
  "browserslist": {
    "default": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 opera versions",
      "last 2 edge versions",
      "safari >= 11",
      "ios >= 11"
    ],
    "evergreen": [
      "last 1 chrome versions",
      "last 1 firefox versions",
      "last 1 opera versions",
      "last 1 edge versions",
      "safari >= 13",
      "ios >= 13"
    ],
    "latest-chrome": [
      "last 1 chrome versions"
    ]
  },
  "devDependencies": {
    "@aws-cdk/aws-cloudfront": "^1.47.1",
    "@aws-cdk/aws-iam": "^1.47.1",
    "@aws-cdk/aws-lambda": "^1.47.1",
    "@aws-cdk/aws-s3": "^1.47.1",
    "@aws-cdk/core": "^1.47.1",
    "@babel/node": "^7.10.3",
    "@quilted/aws": "^0.2.0",
    "@quilted/sewing-kit-plugins": "^0.2.0",
    "@sewing-kit/cli": "^0.1.6",
    "@sewing-kit/eslint-plugin": "^0.0.14",
    "@types/common-tags": "^1.8.0",
    "@types/execa": "^0.9.0",
    "@types/isomorphic-form-data": "^2.0.0",
    "aws-cdk": "^1.47.1",
    "aws-sdk": "^2.708.0",
    "common-tags": "^1.8.0",
    "compression-webpack-plugin": "^4.0.0",
    "lerna": "^3.22.1",
    "postcss-preset-env": "^6.7.0",
    "raw-loader": "^4.0.1",
    "sewing-kit-plugin-knex": "^0.1.0",
    "sewing-kit-plugin-webpack-virtual-modules": "^0.1.1",
    "webpack-bundle-analyzer": "^4.3.0"
  },
  "resolutions": {
    "@quilted/react-testing": "^0.2.2",
    "@types/vfile-message": "1.0.1",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/experimental-utils": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "typescript": "^4.1.0"
  }
}
