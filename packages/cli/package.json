{
  "name": "@watching/cli",
  "description": "A CLI to develop apps for watch.lemon.tools",
  "version": "2.0.10",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "@watching:registry": "https://registry.npmjs.org"
  },
  "license": "MIT",
  "bin": {
    "watchapp": "./bin/watchapp.mjs"
  },
  "sideEffects": false,
  "exports": {
    "./graphql": {
      "types": "./build/typescript/graphql.d.ts",
      "quilt:source": "./source/graphql.ts",
      "quilt:esnext": "./build/esnext/graphql.esnext",
      "import": "./build/esm/graphql.mjs",
      "require": "./build/cjs/graphql.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "graphql": [
        "./build/typescript/graphql.d.ts"
      ]
    }
  },
  "files": [
    "source",
    "templates",
    "build",
    "bin"
  ],
  "dependencies": {
    "@lemonmade/graphql-live": "^0.1.14",
    "@quilted/cli-kit": "^0.1.9",
    "@quilted/graphql": "^0.4.57",
    "@quilted/polyfills": "^0.2.34",
    "@quilted/request-router": "^0.1.6",
    "@quilted/threads": "^0.1.15",
    "@types/mime": "^2.0.0",
    "@watching/graphql": "workspace:^",
    "@watching/tools": "workspace:^",
    "ansi-escapes": "^4.0.0",
    "arg": "^5.0.0",
    "brotli-size": "^4.0.0",
    "chokidar": "^3.5.0",
    "colorette": "^1.0.0",
    "graphql": "^16.6.0",
    "mime": "^2.5.0",
    "open": "^8.0.0",
    "pkg-dir": "^6.0.0",
    "prettier": "^2.8.0",
    "pretty-bytes": "^5.6.0",
    "prompts": "^2.4.0",
    "rollup": "^3.20.0",
    "strip-ansi": "^6.0.0",
    "ws": "^7.0.0"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/prompts": "^2.0.0",
    "@types/ws": "^7.0.0",
    "@watching/clips": "workspace:*",
    "@watching/clips-preact": "workspace:*",
    "@watching/clips-react": "workspace:*",
    "@watching/clips-svelte": "workspace:*"
  },
  "peerDependencies": {
    "@watching/clips": "^0.4.7"
  },
  "peerDependenciesMeta": {
    "@watching/clips": {
      "optional": true
    }
  },
  "eslintConfig": {
    "extends": [
      "@quilted/eslint-config/project"
    ],
    "env": {
      "node": true
    }
  }
}
