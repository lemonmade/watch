{
  "name": "@watching/webpack-hot-worker",
  "version": "0.1.1",
  "publishConfig": {
    "access": "public",
    "@watching:registry": "https://registry.npmjs.org"
  },
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    "./server": {
      "esnext": "./server.esnext",
      "import": "./server.mjs",
      "require": "./server.js"
    },
    "./webpack": {
      "esnext": "./webpack.esnext",
      "import": "./webpack.mjs",
      "require": "./webpack.js"
    },
    "./websocket": {
      "esnext": "./websocket.esnext",
      "import": "./websocket.mjs",
      "require": "./websocket.js"
    },
    "./worker": {
      "esnext": "./worker.esnext",
      "import": "./worker.mjs",
      "require": "./worker.js"
    }
  },
  "dependencies": {
    "strip-ansi": "^6.0.0",
    "webpack": "^5.0.0",
    "ws": "^7.0.0"
  },
  "devDependencies": {
    "@types/webpack": "^5.0.0"
  }
}
