{
  "name": "@watching/graphql",
  "version": "0.0.1",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "@watching:registry": "https://registry.npmjs.org"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "quilt:source": "./source/index.ts",
      "quilt:esnext": "./build/esnext/index.esnext",
      "import": "./build/esm/index.mjs",
      "require": "./build/cjs/index.cjs"
    },
    "./server": {
      "quilt:source": "./source/server/index.ts",
      "quilt:esnext": "./build/esnext/server/index.esnext",
      "import": "./build/esm/server/index.mjs",
      "require": "./build/cjs/server/index.cjs"
    }
  },
  "typesVersions": {
    ">=1": {
      "server": [
        "./build/typescript/server/index.d.ts"
      ],
      "*": [
        "./build/typescript/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "eslintConfig": {
    "extends": "@quilted/eslint-config/project"
  },
  "dependencies": {
    "graphql": "^16.4.0"
  }
}
